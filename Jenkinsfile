pipeline {
  agent any
  stages {
    stage('BUILD') {
      steps {
        echo 'Hi This is Build Environment'
      }
    }
    stage('TESTING') {
      steps {
        echo 'HI This is Testing Environment'
      }
    }
    stage('DEPLOYMENT') {
      steps {
        echo 'Hi This is Deployment Environment'
      }
    }
  }
}